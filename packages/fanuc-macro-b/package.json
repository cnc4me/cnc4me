{
  "name": "@cnc4me/fanuc-macro-b",
  "version": "1.0.0",
  "private": true,
  "description": "Virtual implementation of a fanuc gcode interpreter and execution environment",
  "license": "MIT",
  "author": "Kevin Hill",
  "main": "src/index.ts",
  "scripts": {
    "build": "rollup -c",
    "diagram": "ts-node-esm --swc ./scripts/generateDiagram.ts",
    "events": "tsnd --respawn ./demo/events.demo.ts",
    "insights": "cross-env DEBUG=macro:* tsnd --respawn ./demo/insights.demo.ts",
    "repl": "tsnd ./demo/repl.demo.ts",
    "runtime": "tsnd --respawn ./demo/runtime.demo.ts",
    "runtime:debug": "cross-env DEBUG=macro:* tsnd --respawn ./demo/runtime.demo.ts",
    "test": "jest",
    "test:mem": "jest MacroMemory",
    "types": "ts-node-esm --swc ./scripts/generateDts.ts"
  },
  "dependencies": {
    "chevrotain": "^10.1.2",
    "debug": "^4.3.4",
    "emittery": "^0.10.1",
    "lodash": "^4.17.21",
    "ts-pattern": "^3.3.5"
  },
  "devDependencies": {
    "@chevrotain/types": "^10.1.2",
    "@cnc4me/chevrotain-types-generator": "workspace:*",
    "@cnc4me/jest-matchers": "workspace:*",
    "@swc/core": "^1.2.189",
    "@swc/jest": "^0.2.20",
    "@types/chance": "^1.1.3",
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.180",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "chance": "^1.1.8",
    "cross-env": "^7.0.3",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^27.5.1",
    "jest-extended": "^2.0.0",
    "prettier": "^2.6.0",
    "rollup": "^2.74.0",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-swc": "^0.2.1",
    "ts-node": "^10.8.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  }
}
